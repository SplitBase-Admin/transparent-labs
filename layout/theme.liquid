    <script>
      (function () {
        function toggleReviewFormClasses() {
          const formWrapper = document.querySelector(".jdgm-review-widget .jdgm-form-wrapper");
          if (formWrapper) {
            const isOpen = formWrapper.classList.contains("form-open");
            formWrapper.classList.toggle("form-open", !isOpen);
            document.body.classList.toggle("form-open", !isOpen);
          }
        }
    
        function closeReviewFormClasses() {
          const formWrapper = document.querySelector(".jdgm-review-widget .jdgm-form-wrapper");
          if (formWrapper) {
            formWrapper.classList.remove("form-open");
            document.body.classList.remove("form-open");
          }
        }
    
        function bindReviewEvents() {
          const writeBtn = document.querySelector(".jdgm-review-widget .jdgm-write-rev-link");
          const formWrapper = document.querySelector(".jdgm-review-widget .jdgm-form-wrapper");
    
          if (!writeBtn || !formWrapper || writeBtn.dataset.bound === "true") return;
    
          writeBtn.dataset.bound = "true";
    
          // Toggle on write review click
          writeBtn.addEventListener("click", function () {
            setTimeout(() => toggleReviewFormClasses(), 100);
          });
    
          // Cancel review â€” remove class
          document.body.addEventListener("click", function (e) {
            if (e.target.matches(".jdgm-review-widget .jdgm-form-wrapper .jdgm-cancel-rev")) {
              closeReviewFormClasses();
            }
            // Pseudo-element trigger via parent
            if (e.target.matches(".jdgm-review-widget .jdgm-form .jdgm-form__title")) {
              const cancelBtn = document.querySelector(".jdgm-review-widget .jdgm-form-wrapper .jdgm-cancel-rev");
              if (cancelBtn) cancelBtn.click();
            }
          });
        }
    
        // Wait for Judge.me widget
        const observer = new MutationObserver(() => {
          const writeBtn = document.querySelector(".jdgm-review-widget .jdgm-write-rev-link");
          const formWrapper = document.querySelector(".jdgm-review-widget .jdgm-form-wrapper");
          if (writeBtn && formWrapper) {
            bindReviewEvents(); 
            observer.disconnect();
          }
        });
    
        observer.observe(document.body, { childList: true, subtree: true });
      })();

      {% if cart.item_count > 0 %}
      if (sessionStorage.getItem("open-cart") === "true") {
        const waitForOpenCartMenu = setInterval(() => {
          if (typeof openCartMenu === 'function') {
