<section class="TL_newProdCards">
		<h2>Explore Our Products</h2>



		<div class="tl_prod_carousel tl_prod_carousel-main" data-flickity='{ "contain": true, "pageDots": false, "prevNextButtons": false, "cellAlign": "left", "draggable": false }'>
			<div class="tl_prod_carousel-cell" data-flickity='{ "contain": true, "pageDots": false, "prevNextButtons": false, "cellAlign": "left", "draggable": true }'>
				
              
              
          {% for product in collections['bestsellers'].products limit: 4 %}
              
              {%- liquid
  assign current_variant = product.selected_or_first_available_variant
  assign short_title = product.metafields.product_details.title
  assign subtitle = product.metafields.product_details.subtitle
  
-%}

                {% assign placeholder_size = '200x' | append: placeholder_height %}

              
				<!-- THIS IS CARD 1 -->


				<div class="tl_ProdCards_inner_card">
                  <a href="{{ product.url }}" class="imageLink">
					<div class="tl_ProdCards_inner_card_image">
                       {% render 'lazy-image',
            height: 240,
            image: product.featured_image,
            width: 240,
          %}
					</div>
                  </a>


					<div class="tl_ProdCards_inner_card_text">
						<div class="tl_ProdCards_tag">
							Bestseller
						</div>

    

						<div class="tl_ProdCards_inner_card_text_inner">
							<h3>{{ short_title }}</h3>
							<!-- Replace with ratings code -->


							<div class="tl_ProdCards_rating">
								<div class="tl_ProdCards_rating_stars">
									<div class="flex mb-2 space-x-1 sm:mb-0">
            {%- assign review_average = product.metafields.yotpo.reviews_average | times: 1 -%}
