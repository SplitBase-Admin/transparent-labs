        ]);
    });

    // Update all creatine containers
    creatineContainers.forEach(container => {
        updateServingInfo(container, [
            { html: '<span style="width: 100%;"><p><strong><span style="color: rgb(34, 34, 34)">30 </span></strong><span style="color: rgb(34, 34, 34)">Servings</span></p></span>', size: '30sv' },
            { html: '<span style="width: 100%;"><p><strong><span style="color: rgb(34, 34, 34)">60 </span></strong><span style="color: rgb(34, 34, 34)">Servings</span></p></span>', size: '60sv' },
            { html: '<span style="width: 100%; text-align:center;"><p><span style="color: rgb(34, 34, 34)">Travel Pack </span></p><p><strong><span style="color: rgb(34, 34, 34)">15 </span></strong><span style="color: rgb(34, 34, 34)">Servings</span></p></span>', size: 'Travel Pack (15 ct.)' }
        ]);
    });

hydrateContainers.forEach(container => {
