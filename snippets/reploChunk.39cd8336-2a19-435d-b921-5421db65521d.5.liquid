        ]);
    });

hydrateContainers.forEach(container => {
        const sizeContainer = container.querySelector('div[data-selector="size"]');
        const firstChild = sizeContainer.children.item(0);
        const secondChild = sizeContainer.children.item(1);
        if(firstChild.innerText == '30 Stick Packs' ||  firstChild.servingSize == '30 Stick Packs'){
            firstChild.firstChild.innerHTML ='<span style="width: 100%; text-align:center;"><p><span style="color: rgb(34, 34, 34)">Stick Pack </span></p><p><strong><span style="color: rgb(34, 34, 34)">30 </span></strong><span style="color: rgb(34, 34, 34)">Servings</span></p></span>'
            firstChild.servingSize = '30 Stick Packs'
            if(secondChild){
                secondChild.firstChild.innerHTML ='<span style="width: 100%; text-align:center;"><p><span style="color: rgb(34, 34, 34)">Tub </span></p><p><strong><span style="color: rgb(34, 34, 34)">40 </span></strong><span style="color: rgb(34, 34, 34)">Servings</span></p></span>'
