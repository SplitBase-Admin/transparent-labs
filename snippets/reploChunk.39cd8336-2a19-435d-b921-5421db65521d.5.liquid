    function fixServing() {
    // Select all wpi, creatine, and hydrate containers
    const wpiContainers = document.querySelectorAll('div[data-selector="wpi-container"]');
    const creatineContainers = document.querySelectorAll('div[data-selector="creatine-container"]');
    const hydrateContainers = document.querySelectorAll('div[data-selector="hydrate-container"]');

    // Function to update the serving information for a given container
    function updateServingInfo(container, servingData) {
        const sizeContainer = container.querySelector('div[data-selector="size"]');
        servingData.forEach((data, index) => {
            if(sizeContainer)
            {
              if(sizeContainer.children.item(index)){
                sizeContainer.children.item(index).firstChild.innerHTML = data.html;
                sizeContainer.children.item(index).servingSize = data.size;
              }
            }
        });
    }

    // Update all wpi containers
    wpiContainers.forEach(container => {
        updateServingInfo(container, [
            { html: '<span style="width: 100%;"><p><strong><span style="color: rgb(34, 34, 34)">30 </span></strong><span style="color: rgb(34, 34, 34)">Servings</span></p></span>', size: '2LB' },
