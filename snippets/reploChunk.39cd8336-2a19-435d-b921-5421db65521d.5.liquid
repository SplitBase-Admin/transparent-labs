    function gainerAddToCart(){
        document.querySelectorAll('button[data-button-id="gainer-cart"]').forEach((button)=>{
            button.onclick = () => {
                const getVariants = () => {
                    const variantsNodes = button.closest('div[data-selector="mass-gainer-container"]').querySelectorAll('select[data-custom-list="variants"] > option');
                    console.log(variantsNodes);
                    let variants = {
                        get: function (prop) {
                            if (!this[prop]) return null
                            return this[prop]
                        }
                    };

                    if (variantsNodes.length === 0) return variants

                    variantsNodes.forEach(variantNode => {
                        const key = variantNode?.innerHTML
                        if (typeof key === 'string') {
                            const formattedKey = key.replace(/[\s]+/g, '');
                            Object.assign(variants, { [formattedKey]: variantNode.value })
                        }
                    })

                    return variants;
                }

                const getVariantId = (flavor) => {
                    const variants = getVariants();
                    console.log(variants);
                    const key = `${flavor}`.replace(/[\s]+/g, '');
                    return variants.get(key);
                }

                (async () => {
                    
                    const flavor = button.closest('div[data-selector="mass-gainer-container"]').querySelector('select[data-selector="flavor"]')?.value;
                    const variantId = getVariantId(flavor);

                    if (!variantId) {
                        alert('An error has occurred!!!. Try again later')
                        return;
                    }

                    const selling_plan = button.closest('div[data-selector="mass-gainer-container"]').querySelector('select[data-selector="freq"]')?.value;
                    const radios = button.closest('div[data-selector="mass-gainer-container"]').querySelectorAll('input[type="radio"]')
                    const subSave = radios[0]?.checked
                    let item = [];
                    if (subSave){
                        item.push({ id: variantId, quantity: 1, selling_plan });
                    } else {
                        item.push({ id: variantId, quantity: 1});
                    }
                    await addToCart(item);
                    openCartMenu();
                })()

                async function addToCart(items) {
                    const formData = {
                        items,
                        sections:
                            'cart-notification-product,cart-notification-button,cart-icon-bubble'
                    };
                    console.log(formData);
                    const options = {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify(formData)
                    };
                    const shopifyHost = window.Shopify?.routes.root || '';
                    const requestUrl = shopifyHost + 'cart/add.js';
                    await fetch(requestUrl, options).catch(error => console.error(error));
                    $.getJSON('/cart.js', function (cart) {
                        if (cart.item_count < 1) {
                            $('#cart-count').html("");
                        }
                        else {
                            $('#cart-count').html("<number class='number'>" + cart.item_count + "</number>");
                        }
                    });
                };

            }
        })
    }

    setInterval(wpiAddToCart, 1000);
    setInterval(creatineAddToCart, 1000);
    setInterval(hydrateAddToCart, 1000);
    setInterval(bulkAddToCart, 1000);
    setInterval(leanAddToCart, 1000);
    setInterval(prebioticAddToCart, 1000);
    setInterval(proteinbarAddToCart, 1000);
    setInterval(gainerAddToCart, 1000);


