.AU_content {
    display: flex;
    flex-direction: row;
    width: 100%;
    max-width: 1480px;
    margin: 0 auto;
    margin-top:50px;
}

.AU_content h1 {
    font-size: 44px;
    line-height: 48px;
    margin-top: 0;
    margin-bottom: 25px;
}

 .AU_content_main_section h2 {
    font-size: 24px;
    margin-bottom: 25px;
}

.AU_content_sidebar_box h3 {
    text-transform: uppercase;
}

.AU_content p {
    font-size: 16px;
    line-height: 22px;
    -webkit-font-smoothing: antialiased;
}

.AU_content_main_hero {
    width: 100%;
    max-width: 1480px;
    margin: 0 auto;
    background: url(https://cdn.shopify.com/s/files/1/0866/7664/files/AU_hero.jpg?v=1663159395);
    flex: 0 0 100%;
    background-size: cover;
    background-position: center;
    margin-bottom: 50px;
    min-height: 450px;
    border-radius: 6px;
}

.AU_content_main {
width:70%;
}

.AU_content_sidebar {
    min-height: 200px;
    width: 30%;
    padding-left: 60px;
    align-self: flex-start;
    position: sticky;
    top: 50px;
    padding-bottom: 50px;
}

.AU_content_sidebar_box {
    background: #ffffff;
    padding: 20px;
    border-radius: 4px;
    border: 1px solid #F0F0F0;
    text-decoration: none;
}

.AU_content_sidebar_box ul {
    list-style:none;
    padding-left:0px;
}

.AU_content_sidebar_box ul li a {
    text-decoration: none;
    color: inherit;
    position: relative;
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 20px;
}



.AU_content_sidebar_box ul li {
    padding: 15px 0px;
    border-bottom: 1px solid #efefef;
    display: flex;
    fill: rgb(19, 40, 54);
    justify-content: flex-start;
    align-items: flex-start;
    position: relative;
}

.AU_content_sidebar_box ul li svg {
    fill: #132836;
    height: 24px;
    padding-left: 20px;
    transition: ease all .5s;
}

.AU_content_sidebar_box ul li:last-of-type {
    border-bottom: 0px solid #efefef;
    padding-bottom: 0px;
}

.AU_content_sidebar_box_alt {
    background: rgb(19, 40, 54);
    padding: 20px;
    border-radius: 4px;
    border: 1px solid rgb(19, 40, 54);
    color: #ffffff;
    margin-top: 40px;
    text-transform: uppercase;
    display: flex;
    fill: #ffffff52;
    font-size: 14px !important;
    position: relative;
    padding-right: 100px;
    transition: ease all .25s;
    margin-bottom:40px;
}

.AU_content_sidebar_box_alt_link  {
 text-decoration:none;
}

.AU_content_sidebar_box_alt img {
    position: absolute;
    right: 0;
    width: 125px;
    top: 7px;
    transition: ease all .25s;
}

.AU_content_sidebar_box_alt h3 {
    color: rgb(255 255 255);
    font-size: 18px;
    line-height: 20px;
    max-width: 80%;
}

.AU_content_sidebar_box_alt svg {
    width: 12px;
    height: 16px;
    margin-left: 5px;
}

.AU_content_sidebar_box_alt:hover {
    background: rgb(64 181 232);
    border: 1px solid rgb(64 181 232);
    transition: ease all .25s;
}

.AU_content_sidebar_box_alt:hover img {
    transform: rotate(3deg);
    transition: ease all .25s;
}

.AU_content_main_section {
    margin-top: 60px;
    margin-bottom:60px;
}
.AU_content_main_section video {
    margin-bottom: 30px;
}


@media only screen and (max-width: 800px) {
.AU_content {
flex-direction:column;
    margin-top:30px;
}
.AU_content_main, .AU_content_sidebar {
    width: auto;
    padding: 20px;
}
.AU_content_main_hero {
    min-height: 200px;
        margin-bottom: 30px;
}

.AU_content h1 {
    font-size: 34px;
    line-height: 40px;
}

.AU_content_main_section {
    margin-top: 30px;
    margin-bottom:0px;
}


}

</style>



