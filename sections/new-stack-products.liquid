                {% assign pure_product = all_products[pro_handle] %}

                {% if pure_product.available %}
                  <div class="items stack-item">
                    <div class="items-img">
                      {% assign pure_product_featured_image = product.featured_image | split: '/' | last | split: '.' | first | replace: '/', ' ' | replace: '-', ' ' | replace: '_', ' ' %}
                      <img src="{{ pure_product.featured_image | img_url:'' }}" alt="{% if pure_product.featured_image.alt %}{{ pure_product.featured_image.alt }}{% else %}{{ pure_product_featured_image }}{% endif %}" loading="lazy">
                    </div>
                    <div class="stack-text">
                      <h3>{{ pure_product.title }}</h3>

                      {% if pure_product.options[0] %}
                        {% assign used = '' %}

                        <div class="input-wraper">
                          <label for="select-one_{{pure_product.id}}">{{ pure_product.options[0] }}</label>

                          <select
                            data-option-position="1"
                            id="select-one_{{pure_product.id}}"
                            onchange="letsDoThis_{{pure_product.id}}()"
                          >
                            {% for variant in pure_product.variants %}
                              {% unless variant.metafields.secomapp.freegifts %}
                                {% unless variant.title contains '(Freegifts)' %}
                                  {% unless variant.inventory_quantity == 0 %}
                                    {% unless used contains variant.option1 %}
                                      <option value="{{ variant.option1 }}">{{ variant.option1 }}</option>
                                      {% capture used %}{{ used }} {{ variant.option1 }}{% endcapture %}
                                    {% endunless %}
                                  {% endunless %}
                                {% endunless %}
                              {% endunless %}
                            {% endfor %}
                          </select>
                        </div>
                      {% endif %}

                      {% if pure_product.options[1] %}
                        <div class="input-wraper">
                          {% assign used = '' %}
                          <label for="select-one_{{pure_product.id}}">{{ pure_product.options[1] }}</label>
                          <select id="select-two_{{pure_product.id}}" onchange="letsDoThis_{{pure_product.id}}()">
                            {% for variant in pure_product.variants %}
                              {% unless variant.metafields.secomapp.freegifts %}
                                {% unless variant.title contains '(Freegifts)' %}
                                  {% unless variant.inventory_quantity == 0 %}
                                    {% unless used contains variant.option2 %}
                                      <option value="{{ variant.option2 }}">{{ variant.option2 }}</option>
                                      {% capture used %}{{ used }} {{ variant.option2 }}{% endcapture %}
                                    {% endunless %}
                                  {% endunless %}
                                {% endunless %}
                              {% endunless %}
                            {% endfor %}
                          </select>
                        </div>
                      {% endif %}

                      {% if pure_product.options[2] %}
