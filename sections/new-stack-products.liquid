                          <select id="select-three" onchange="letsDoThis_{{pure_product.id}}()">
                            {% for variant in pure_product.variants -%}
                              \
                              {% unless variant.metafields.secomapp.freegifts %}
                                {% unless variant.title contains '(Freegifts)' %}
                                  {% unless variant.inventory_quantity == 0 %}
                                    {% unless used contains variant.option3 %}
                                      <option value="{{ variant.option3 }}">{{ variant.option3 }}</option>
                                      {% capture used %}{{ used }} {{ variant.option3 }}{% endcapture %}
                                    {% endunless %}
                                  {% endunless %}
                                {% endunless %}
                              {% endunless %}
                            {% endfor %}
                          </select>
                        </div>
                      {% endif %}

                      <div class="p1_selling_plan_{{forloop.index}}"></div>
                      <input
                        type="hidden"
                        name="id"
                        id="product-select_{{pure_product.id}}"
                        class="stack_vid"
                        value="{{ pure_product.selected_or_first_available_variant.id }}"
                      >
                    </div>
                    <script>
                                    function letsDoThis_{{pure_product.id}}()
                                    {
                      				{% if pure_product.options[0] %}
                                      	var opt1 = document.getElementById('select-one_{{pure_product.id}}').value;
                                      {% endif %}
                      				{% if pure_product.options[1] %}
                                      	var opt2 = document.getElementById('select-two_{{pure_product.id}}').value;
                      				{% endif %}
                                      {% if pure_product.options[2] %}
                                      	var opt3 = document.getElementById('select-three_{{pure_product.id}}').value;
                      				{% endif %}
                                      var id = '';
                                      {% for v in pure_product.variants %}
                      {% unless v.metafields.secomapp.freegifts %}
                      {% unless v.title contains '(Freegifts)' %}

                      					{% unless v.inventory_quantity == 0 %}
                                      		if(opt1=="{{ v.option1 }}"{% if pure_product.options[1] %} && opt2=="{{ v.option2 }}"{% endif %}{% if pure_product.options[2] %} && opt3=="{{ v.option3 }}"{% endif %})
                                          	{
                                        			var id = {{ v.id }};
                                        			var price = "{{ v.price | money }}";
                                      		}
                                      	{% endunless %}

                      {%endunless%}
                      {%endunless%}
                                      {% endfor %}
                                      if(id!='') {
                                        document.getElementById('product-select_{{pure_product.id}}').value = id;
                                                                document.getElementById('price').innerHTML = price;
                                      } else {
                                        document.getElementById('product-select_{{pure_product.id}}').value = '';
                                                                document.getElementById('price').innerHTML = 'Unavailable';
                                      }

                                    }
                    </script>
                    <script type="text/javascript">
                    var variant_values = {{ pure_product.variants | json }};
                var product_json = {{ pure_product | json }};

                //console.log(variant_values);
                var len = variant_values.length;
                console.log('product json: ',product_json)
                var rechargeSellingPlanGroup = {{ pure_product.selling_plan_groups | where: "app_id", "SKIO" | json }}
                for(var i = 0; i < len; i++)
                {
                  console.log('variant_values: ',i,' ',variant_values[i])
                    //console.log(variant_values[i].id);
                    var plans = variant_values[i].selling_plan_allocations.filter(allocation => allocation.selling_plan_group_id == rechargeSellingPlanGroup[0].id)

                    var len2 = plans.length;
                    for(var j=0; j < len2; j++)
                          {
                            $(".p1_selling_plan_{{forloop.index}}").append('<input type=hidden data-stack="{{ product.id }}" data-price='+ Shopify.formatMoney(plans[j].price) +' data-otp= '+ Shopify.formatMoney(plans[j].compare_at_price)+'  class="selling_plan_'+j+'" name="selling_plan" value="'+plans[j].selling_plan_id+'">');
                              console.log(plans);
                          }
                          if(i == 0)
                          {
                          	break;
                          }

                      }
                    </script>
                  </div>
                {% endif %}
              {% endfor %}
