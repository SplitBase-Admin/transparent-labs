<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mathjs/6.2.5/math.min.js"></script> 
<script>  
  
    document.addEventListener('DOMContentLoaded', function() {  

      
       var firstTab = document.querySelector('#tabs-nav li:first-child');
       if (firstTab) {
         firstTab.classList.add('active');
       }
       
       var tabContents = document.querySelectorAll('.tab-content');
       tabContents.forEach(function(content) {
         content.style.display = 'none';
       });
       
       var firstTabContent = document.querySelector('.tab-content:first-of-type');
       if (firstTabContent) {
         firstTabContent.style.display = 'block';
       }

       // Click function
       var tabNavItems = document.querySelectorAll('#tabs-nav li');
       tabNavItems.forEach(function(item) {
         item.addEventListener('click', function(e) {
           e.preventDefault();
           
           // Remove active class from all tabs
           document.querySelectorAll('#tabs-nav li a').forEach(function(link) {
             link.classList.remove('active');
           });
           
           // Add active class to clicked tab
           var link = this.querySelector('a');
           if (link) {
             link.classList.add('active');
           }
           
           // Hide all tab contents
           document.querySelectorAll('.tab-content').forEach(function(content) {
             content.style.display = 'none';
           });
           
           // Show active tab content
           var activeTab = link.getAttribute('href');
           var activeContent = document.querySelector(activeTab);
           if (activeContent) {
             activeContent.style.display = 'block';
           }
         });
       });
        
        function GetParameterValues(param) {  
            var url = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');  
            for (var i = 0; i < url.length; i++) {  
