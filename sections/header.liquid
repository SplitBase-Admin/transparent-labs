      var Accordion = function(el, multiple) {
        this.el = el || {};
        this.multiple = multiple || false;

        // Variables privadas
        var links = this.el.find('.menu-item-has-children-m > a');
        // Evento
        links.on('click', {el: this.el, multiple: this.multiple}, this.dropdown)
      }

      Accordion.prototype.dropdown = function(e) {
        var $el = e.data.el;
        $this = $(this),
          $next = $this.next();

        $next.slideToggle(500);
        $this.parent().toggleClass('open');

        if (!e.data.multiple) {
          $el.find('.menu-subs-m').not($next).slideUp(100).parent().removeClass('open');
        };
      }

      var accordion = new Accordion($('.menu-section-m'), false);
    });



    $(document).on("click",".menu-mobile-close",function() {

      $(".menu").removeClass('active');
      $(".overlay").removeClass('active')

    });

  });
