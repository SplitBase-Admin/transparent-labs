document.addEventListener("DOMContentLoaded", menuTransitionEffect);

function menuTransitionEffect() {
    let menuItems = document.querySelectorAll(".main--menu-items");
    let menuWrapper = document.querySelector(".new-megamenu-wrapper");
    let firstLoad = true;
    let activeItem = null; // Track the currently active menu item

    if (menuItems.length === 0 || !menuWrapper) return; // Prevent errors if elements are missing

    menuItems.forEach(item => {
        item.addEventListener("mouseenter", function () {
            if (firstLoad) {
                // If it's the first hover, add the effect and keep it until another item is hovered
                if (activeItem) {
                    activeItem.classList.remove("first-load");
                }
                item.classList.add("first-load", "show");
                activeItem = item;

                setTimeout(() => {
                    firstLoad = false;
                }, 500);
            } else {
                // After the first time, just show the menu without effect
                item.classList.add("no-transition", "show");
                if (activeItem) {
                    activeItem.classList.remove("first-load");
                }
                activeItem = item;
            }
        });
    });

    // Detect when user completely leaves the menu area
    document.addEventListener("mousemove", function (e) {
        if (!menuWrapper.contains(e.target) && ![...menuItems].some(el => el.contains(e.target))) {
            menuItems.forEach(item => {
                item.classList.remove("show", "no-transition", "first-load");
            });
            activeItem = null;
            firstLoad = true; // Reset first-load effect when completely leaving
        }
    });
}



</script>

<style>

  .main--menu-items.first-load.show .new-megamenu-wrapper {
    transition: all 0.2s ease-in-out;
  }
  
  .main--menu-items.no-transition .new-megamenu-wrapper {
    transition: none !important;
  }
  
</style>
